{
  "id": "/apache-ignite/data-grid",
  "cmd": null,
  "cpus": 1,
  "mem": 1024,
  "disk": 200,
  "instances": 1,
  "acceptedResourceRoles": [
    "*"
  ],
  "container": {
    "type": "DOCKER",
    "docker": {
      "forcePullImage": false,
      "image": "codeunited/ignite-showcase:1.0.0",
      "parameters": [],
      "privileged": false
    },
    "volumes": []
  },
  "env": {
    "HTTP_PORT": "${PORT0}"
  },
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "ignoreHttp1xx": false,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 3,
      "path": "/health",
      "portIndex": 0,
      "protocol": "HTTP",
      "timeoutSeconds": 20,
      "delaySeconds": 15
    }
  ],
  "labels": {
    "HAPROXY_GROUP": "external",
    "HAPROXY_0_VHOST": "dg.127.0.0.1.xip.io"
  },
  "portDefinitions": [
    {
      "port": 10000,
      "protocol": "tcp"
    }
  ]
}